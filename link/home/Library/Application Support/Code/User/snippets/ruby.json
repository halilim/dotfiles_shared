{
	"#{}": {
		"prefix": "#",
		"body": "#{$0}",
		// "scope": "string.quoted.double.interpolated.ruby"
	},

	"Array.new": {
		"prefix": "an",
		"body": "Array.new($0)",
	},

	"ap(k: v)": {
		"prefix": "ap",
		"body": "ap(${1:var_name}: ${2:var_value})",
	},

	"begin block": {
		"prefix": "beg",
		"body": [
			"begin",
				"\t${1:code}",
			"end"
		],
	},

	"begin ... rescue": {
		// cSpell:disable-next-line
		"prefix": "begr",
		"body": [
			"begin",
				"\t${1:code}",
			"rescue ${2:StandardError} => e",
				"\t${3:rescue_code}",
			"end",
		],
	},

	"block_given?": {
		"prefix": "bg",
		"body": "block_given?",
	},

	"binding.break": {
		"prefix": "bb",
		"body": [
			"binding.break",
		],
	},

	"binding.irb": {
		"prefix": "bi",
		"body": "binding.irb",
	},

	"binding.pry": {
		"prefix": "bp",
		"body": "binding.pry",
	},

	"binding.remote_pry": {
		"prefix": "bpr",
		"body": "binding.remote_pry",
	},

	"Benchmark.bmbm": {
		"prefix": "bmbm",
		"body": [
			"Benchmark.bmbm do |x|",
				"\tx.report('${1:name}') { ${2:code} }",
			"end"
		],
	},

	"Benchmark.measure": {
		"prefix": "bmm",
		"body": [
			"Benchmark.measure do",
				"\t$0",
			"end"
		],
	},

	"{ |e| ... }": {
		"prefix": "b",
		"body": "{ |${1:e}| $0 }",
	},

	"class << self": {
		// cSpell:disable-next-line
		"prefix": "clase",
		"body": [
			"class << self",
				"\t$0",
			"end"
		],
	},

	"Class = Struct.new(...) do": {
		// cSpell:disable-next-line
		"prefix": "clast",
		"body": [
			"${1:${TM_FILENAME/(?:\\A|_)([A-Za-z0-9]+)(?:\\.rb)?/(?2::\\u$1)/g}} = Struct.new(${2:vars}) do",
				"\t${0}",
			"end"
		],
	},

	"def initialize": {
		"prefix": "defi",
		"body": [
			"def initialize${1:($2)}",
				"\t$0",
			"end"
		],
	},

	"define_method": {
		// cSpell:disable-next-line
		"prefix": "defm",
		"body": [
			"define_method ${1:name} do |${2:params}|",
				"\t$0",
			"end",
		]
	},

	"def ... rescue": {
		// cSpell:disable-next-line
		"prefix": "defr",
		"body": [
			"def ${1:method_name(params)}",
				"\t${4:code}",
			"rescue ${5:StandardError} => e",
				"\t${6:rescue_code}",
			"end"
		],
	},

	"def self. ...": {
		"prefix": "defs",
		"body": [
			"def self.${1:method_name(params)}",
				"\t$0",
			"end"
		],
	},

	"do": {
		"prefix": "do",
		"body": [
			"do",
				"\t$0",
			"end"
		],
	},

	"do |v|": {
		"prefix": ["doo", "dov"],
		"body": [
			"do |${1:variable}|",
				"\t$0",
			"end"
		],
	},

	"each do |e|": {
		"prefix": "ead",
		"body": [
			"each do |${1:e}|",
				"\t$0",
			"end"
		],
	},

	"each do |k, v|": {
		// cSpell:disable-next-line
		"prefix": "eadh",
		"body": [
			"each do |${1:k}, ${2:v}|",
				"\t$0",
			"end"
		],
	},

	"each_with_object": {
		"prefix": "ewo",
		"body": [
			"each_with_object(${1:obj}) do |${2:e}, ${3:memo_obj}|",
				"\t$3$0",
			"end"
		],
	},

	"each_with_object for hash": {
		// cSpell:disable-next-line
		"prefix": "ewoh",
		"body": [
			"each_with_object(${1:obj}) do |(${2:k}, ${3:v}), ${4:memo_obj}|",
				"\t$4$0",
			"end"
		],
	},

	"frozen_string_literal": {
		"prefix": "fro",
		"body": [
			"# frozen_string_literal: true",
			""
		],
	},

	"squiggly heredoc": {
		"prefix": "hs",
		"body": [
			"<<~${1:HEREDOC}",
				"\t$0",
			"$1"
		],
	},

	"I18n.t": {
		"prefix": "i8t",
		"body": "I18n.t('$0')",
	},

	"is_a?": {
		"prefix": "is",
		"body": "is_a?($0)",
	},

	"->(...) { ... }": {
		"prefix": "l",
		"body": "->(${1:params}) { $0 }",
	},

	"lambda do": {
		"prefix": "ll",
		"body": [
			"lambda do |${1:params}|",
				"\t$0",
			"end"
		],
	},

	"map do": {
		// cSpell:disable-next-line
		"prefix": "mapd",
		"body": [
			"map do |${1:e}|",
				"\t$0",
			"end"
		],
	},

	"matching methods": {
		"prefix": "mm",
		"body": "methods.select { |m| m.to_s =~ /${1:pattern}/i }",
	},

	"public methods": {
		"prefix": "pm",
		"body": "public_methods.reject { |m| m.to_s.start_with?('_') } - Object.public_methods",
	},

	"module_eval": {
		"prefix": "me",
		"body": [
			"module_eval <<~RUBY, __FILE__, __LINE__ + 1",
				"\t$0",
			"RUBY"
		],
	},

	"# :nodoc:": {
		"prefix": "nd",
		"body": "# :nodoc:",
	},

	"private send": {
		"prefix": "prs",
		"body": "__send__",
	},

	"public_send": {
		"prefix": "ps",
		"body": "public_send",
	},

	"reduce": {
		"prefix": "red",
		"body": [
			// "reduce${1:(${2:initial})} do |${3:memo}, ${4:item}|",
			// "reduce(${1:initial}) do |${2:memo}, ${3:item}|",
			"reduce${1:($2)} do |$3, $4|",
				"\t$0",
			"end"
		],
	},

	"require": {
		"prefix": "req",
		"body": "require '$1'$0",
	},

	"require_relative": {
		// cSpell:disable-next-line
		"prefix": "reqr",
		"body": "require_relative '${1:file}'",
	},

	"rubocop:disable": {
		// cSpell:disable-next-line
		"prefix": "rudi",
		"body": "# rubocop:disable $0",
	},

	"rubocop:enable": {
		"prefix": "rue",
		"body": "# rubocop:enable $0",
	},

	"tap do": {
		// cSpell:disable-next-line
		"prefix": "tapd",
		"body": [
			"tap do |${1:variable}|",
				"\t${1:variable}$0",
			"end"
		],
	},

	"Time.current": {
		"prefix": "tc",
		"body": "Time.current",
	},

	// Rails

	"errors.add": {
		// cSpell:disable-next-line
		"prefix": "erra",
		"body": "errors.add(:${1:attr}, :${2:message_key})",
	},

	"errors.messages": {
		"prefix": "erm",
		"body": "errors.messages",
	},

	"group pluck": {
		"prefix": "gp",
		"body": [
			"${1:scope}.",
				"\tgroup(:foo_id).",
				"\tpluck(:foo_id, Arel.sql('ARRAY_AGG(bar_id)')).each do |foo_id, bar_ids|",
					"\t\t$0",
				"\tend"
		],
	},

	"logger.debug": {
		// cSpell:disable-next-line
		"prefix": "logd",
		"body": "logger.debug { $0 }",
	},

	"logger.error": {
		"prefix": "loge",
		"body": "logger.error { $0 }",
	},

	"logger.info": {
		// cSpell:disable-next-line
		"prefix": "logi",
		"body": "logger.info { $0 }",
	},

	"logger.warn": {
		// cSpell:disable-next-line
		"prefix": "logw",
		"body": "logger.warn { $0 }",
	},

	"Rails.logger.debug": {
		// cSpell:disable-next-line
		"prefix": "rlogd",
		"body": "Rails.logger.debug { $0 }",
	},

	"Rails.logger.error": {
		// cSpell:disable-next-line
		"prefix": "rloge",
		"body": "Rails.logger.error { $0 }",
	},

	"Rails.logger.info": {
		// cSpell:disable-next-line
		"prefix": "rlogi",
		"body": "Rails.logger.info { $0 }",
	},

	"Rails.logger.warn": {
		// cSpell:disable-next-line
		"prefix": "rlogw",
		"body": "Rails.logger.warn { $0 }",
	},

	"local_assigns": {
		"prefix": "la",
		"body": "local_assigns[:$0]",
	},

	"controller & params in console": {
		// cSpell:disable-next-line
		"prefix": "racp",
		"body": [
			"uri = '${1:URI}'",
			"params = ActionController::Parameters.new(Rack::Utils.parse_nested_query(URI(uri).query))",
			"controller = ${2:Things}Controller.new",
			"# current_user = User.find(${3:user_id})",
			"# controller.define_singleton_method(:current_user) { current_user }",
			"controller.params = params",
			"controller.${4:action}",
		],
	},

	"AR scope": {
		"prefix": "sco",
		"body": "scope :${1:name}, ->${2:(${3:params})} { $0 }",
	},

	"skip auth token": {
		"prefix": "skip_token",
		"body": "skip_before_action :verify_authenticity_token",
	},

	"with_indifferent_access": {
		"prefix": "wia",
		"body": "with_indifferent_access",
	},

	// End: Rails

	// Unit tests & RSpec

	"allow(...).to receive(:...).and_return(...)": {
		// cSpell:disable-next-line
		"prefix": "allr",
		"body": "allow(${1:object}).to receive(:${2:method}).and_return(${0:value})",
	},

	"allow(...).to receive(:...).and_raise(...)": {
		// cSpell:disable-next-line
		"prefix": "allra",
		"body": "allow(${1:object}).to receive(:${2:method}).and_raise(${3:'test error'})",
	},

	"allow(...).to receive(:...).and_call_original": {
		// cSpell:disable-next-line
		"prefix": "allro",
		"body": "allow(${1:object}).to receive(:${2:method}).and_call_original",
	},

	"allow(...).to receive(:...).with(...).and_return(...)": {
		// cSpell:disable-next-line
		"prefix": "allrw",
		"body": "allow(${1:object}).to receive(:${2:method}).with(${3:args}).and_return(${0:value})",
	},

	// https://rspec.info/features/3-13/rspec-mocks/configuring-responses/wrapping-the-original-implementation/
	// https://www.rubydoc.info/github/rspec/rspec-mocks/RSpec%2FMocks%2FMessageExpectation:and_wrap_original
	"allow(...).to receive(...).and_wrap_original do": {
		// cSpell:disable-next-line
		"prefix": "allrwo",
		"body": [
			"allow(${1:object}).to receive(:${2:method}).and_wrap_original do |orig_method, *params, &block|",
				"\torig_method.call(*params, &block)$0",
			"end"
		],
	},

	"assigns[:...]": {
		"prefix": "as",
		"body": "assigns[:${0:name}]",
	},

	"assign(:var, value) - view specs: assign @vars": {
		"prefix": "asv",
		"body": "assign(:${1:var}, ${0:value})",
	},

	"before { ... }": {
		"prefix": "bef",
		"body": "before { $0 }",
	},

	"before do ...": {
		// cSpell:disable-next-line
		"prefix": "befd",
		"body": [
			"before do",
				"\t$0",
			"end"
		],
	},

	"be_falsy": {
		// cSpell:disable-next-line
